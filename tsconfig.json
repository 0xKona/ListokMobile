{
  "extends": "@react-native/typescript-config/tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "strict": true,
    "jsx": "react",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@typed/*": ["typed/*"],
      "@redux/*": ["redux/*"],
      "@app/*": ["app/*"],
      "@root/*": ["./*"],
    }
  },
  "include": ["app/**/*", "typed/**/*", "redux/**/*"]
}
